<form action='/person' method='post' >
  <input name='id' type='hidden' value='<%= person.id %>'/>
  <input name='firstName' placeholder='firstName' value='<%= person.firstName %>'/>
  <input name='lastName' placeholder='lastName' value='<%= person.lastName %>'/>
  <input name='age' placeholder='age' value='<%= person.age %>'/>
  <button type='submit'>提交</button>
</form>
